function t223_setHeight (rec) {
var videoLazy = rec.querySelectorAll('.t-video-lazyload');
if (!videoLazy.length) return;
Array.prototype.forEach.call(videoLazy, function (video) {
var videoIframe = video.querySelector('iframe');
var videoWidth = parseInt(window.getComputedStyle(video).width, 10);
var computedHeight = videoWidth / (16 / 9);
video.style.height = computedHeight + 'px';
if (videoIframe) {
videoIframe.height = computedHeight;
}
setTimeout(function () {
var videoIframe = video.querySelector('iframe');
if (videoIframe) {
videoIframe.height = computedHeight;
}
}, 300);
});
};</script> </div><div id="rec119910204" class="r t-rec t-rec_pt_0 t-rec_pb_0 r_anim r_showed" style="padding-top:0px;padding-bottom:0px; " data-record-type="124"><!-- T116 --><div class="t116"><div class="t-width t-margin_auto"><div class="t-video-lazyload t116__videowrapper" data-videolazy-type="vimeo" data-videolazy-id="178733197" data-videolazy-load="true"><iframe src="./   . _files/178733197.html" width="100%" height="100%" frameborder="0" webkitallowfullscreen="" mozallowfullscreen="" allowfullscreen=""></iframe></div></div></div></div><div id="rec115352316" class="r t-rec t-rec_pt_60 t-rec_pb_60" style="padding-top:60px;padding-bottom:60px; " data-animationappear="off" data-record-type="603"><!-- t603--><div class="t603"><div class="t603__container t-container_100"><div class="t603__tile t603__tile_25" itemscope="" itemtype="http://schema.org/ImageObject"><div class="t603__blockimg t603__blockimg_4-3 t-bgimg loaded" data-original="https://static.tildacdn.com/tild3631-3062-4866-b531-396131326630/4_iK7qJRl5k.jpg" bgimgfield="gi_img__0" data-zoom-target="0" style="background: url(&quot;https://optim.tildacdn.com/tild3631-3062-4866-b531-396131326630/-/resize/500x400/-/format/webp/4_iK7qJRl5k.jpg&quot;) center center / cover no-repeat;" role="img" aria-label="    " data-lazy-rule="comm:resize,round:100"><meta itemprop="image" content="https://static.tildacdn.com/tild3631-3062-4866-b531-396131326630/4_iK7qJRl5k.jpg"> <meta itemprop="caption" content="    "> </div></div><div class="t603__tile t603__tile_25" itemscope="" itemtype="http://schema.org/ImageObject"><div class="t603__blockimg t603__blockimg_4-3 t-bgimg loaded" data-original="https://static.tildacdn.com/tild6564-3930-4330-b638-306437303966/r27_WYscD1E.jpg" bgimgfield="gi_img__1" data-zoom-target="1" style="background: url(&quot;https://optim.tildacdn.com/tild6564-3930-4330-b638-306437303966/-/resize/500x400/-/format/webp/r27_WYscD1E.jpg&quot;) center center / cover no-repeat;" role="img" aria-label="   " d